		"@amoutonbrady/baracuda": "^1.4.0",
		"@arr/find": "^1.0.1",
		"@arr/findindex": "^1.0.1",
		"@arr/map": "^1.0.1",
		"@hyperapp/events": "^0.0.3",
		"@mdi/js": "^4.8.95",
		"hyperapp": "^2.0.3",
		"hyperapp-debug": "^1.0.0-beta.5",
		"hyperapp-fx": "^2.0.0-beta.1",
		"marked": "^0.8.0"
	},
	"devDependencies": {
		"@fullhuman/postcss-purgecss": "^2.0.5",
		"cross-env": "^7.0.0",
