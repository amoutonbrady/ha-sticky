{
	"name": "ha-sticky",
	"version": "0.0.1",
	"description": "A small Hyperapp experiment.",
	"default": "dist/index.html",
	"scripts": {
		"dev": "rimraf dist && cross-env NODE_ENV=development parcel src/index.html",
		"build": "npm rebuild && rimraf dist && cross-env NODE_ENV=production parcel build src/index.html",
		"prettier": "prettier --write \"**/**/*.{js,html,ts,md}\""
	},
	"keywords": [
		"hyperapp",
